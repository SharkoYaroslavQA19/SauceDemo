<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Tests" parallel="classes" thread-count="1">
    <parameter name="browser" value="firefox"/>
    <test name="All Smoke tests">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <packages>
            <package name="Test"/>
        </packages>
    </test>
</suite>